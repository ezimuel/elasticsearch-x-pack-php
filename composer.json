{
    "name": "elasticsearch/xpack",
    "description": "XPack namespace for Elasticsearch-PHP",
    "keywords": ["search", "client", "elasticsearch", "xpack"],
    "type": "library",
    "license": "Apache 2",
    "authors": [
        {
            "name": "Zachary Tong"
        },
        {
            "name": "Enrico Zimuel"
        }
    ],
    "require": {
        "php": "^7.1",
        "elasticsearch/elasticsearch": "6.5.*"
    },
    "require-dev": {
        "doctrine/inflector": "^1.3",
        "phpstan/phpstan-shim": "^0.11",
        "squizlabs/php_codesniffer": "^3.4",
        "cpliakas/git-wrapper": "~2.0",
        "phpunit/phpunit": "^7.5",
        "mustangostang/spyc": "dev-feature/empty-hash-as-object"
    },
    "autoload": {
        "psr-4": {
            "Elasticsearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elasticsearch\\Tests\\XPack\\": "tests/unit",
            "Elasticsearch\\IntegrationTests\\XPack\\": "tests/integration",
            "Elasticsearch\\XPack\\Util\\" : "util/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ezimuel/spyc"
        }
    ]
}
