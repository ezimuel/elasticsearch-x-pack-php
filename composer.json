{
    "name": "elasticsearch/xpack",
    "description": "XPack namespace for Elasticsearch-PHP",
    "keywords": ["search","client", "elasticsearch", "xpack"],
    "type": "library",
    "license": "Apache 2",
    "authors": [
        {
            "name": "Zachary Tong"
        },
        {
            "name": "Enrico Zimuel"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "cpliakas/git-wrapper": "~2.0",
        "phpunit/phpunit": "^7.5",
        "symfony/yaml": "~4.0",
        "elasticsearch/elasticsearch": "^6.3"
    },
    "autoload": {
        "psr-4": {
            "Elasticsearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elasticsearch\\Tests\\": "tests/unit",
            "Elasticsearch\\IntegrationTests\\": "tests/integration"
        }
    }
}
