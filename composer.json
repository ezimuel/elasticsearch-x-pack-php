{
    "name": "elasticsearch/xpack",
    "description": "XPack namespace for Elasticsearch-PHP",
    "keywords": ["search","client", "elasticsearch", "xpack"],
    "type": "library",
    "license": "Apache 2",
    "authors": [
        {
            "name": "Zachary Tong"
        },
        {
            "name": "Enrico Zimuel"
        }
    ],
    "require": {
        "php": "^7.1",
        "elasticsearch/elasticsearch": "^6.3"
    },
    "require-dev": {
        "doctrine/inflector": "^1.3",
        "phpstan/phpstan-shim": "^0.11",
        "squizlabs/php_codesniffer": "^3.4",
        "cpliakas/git-wrapper": "~2.0",
        "phpunit/phpunit": "^7.5",
        "symfony/yaml": "~4.0",
        "symfony/finder": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Elasticsearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elasticsearch\\XPack\\Tests\\": "tests/unit",
            "Elasticsearch\\XPack\\IntegrationTests\\": "tests/integration",
            "Elasticsearch\\XPack\\Util\\" : "util/"
        }
    }
}
